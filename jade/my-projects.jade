doctype html
- var projects = "corrent-nav"
html
    head(lang="ru-RU")
        meta(charset='UTF-8')
        // Просим IE переключить в последний режим
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        // Корректное масштабирование на разных устройствах
        meta(name='viewport', content='width=device-width, initial-scale=1')
        // SEO
        meta(name='discription', content=' ')
        meta(name='keywords', content=' ')
        meta(name='auther', content='Stelios Baglaridis')
        // FAVICON
        link(rel='shortcut icon', href='favicon.ico', type='image/x-icon')
        // CSS
        link(rel='stylesheet', href='/bower/normalize-css/normalize.css')
        link(rel='stylesheet', href='/css/global.css')
        link(rel='stylesheet', href='/css/my-projects.css')
        link(rel='stylesheet', href='/css/bpopup.css')
        link(rel='stylesheet', href='/bower/qtip2/jquery.qtip.css')
        script(src='js/modernizr-custom.js')
        title Мои работы

    body
        // Ваша верстак тут
        .wrapper
            include temp/_header
            .container
                .container-inner.clearfix
                    aside.side-bar
                        include temp/_navigation
                        include temp/_contacts
                    .content
                        include temp/_my-project
                        include temp/_bpopup
        include temp/_footer
    // JS
    script(src='/bower/jquery/dist/jquery.js')
    script(src='bower/jquery-placeholder/jquery.placeholder.js')
    script(src="/bower/bPopup/jquery.bpopup.js")
    script(src="/bower/qtip2/jquery.qtip.js")
    script(src="/js/pop_up_window.js")
    script(src="/js/ajax-requests.js")
    script(src="/js/validator.js")
    script(src="/js/file-upload.js")
